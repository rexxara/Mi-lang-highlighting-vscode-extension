{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "Mi",
	"patterns": [
		{
			"include": "#keywords"
		},
		{
			"include": "#strings"
		},
		{
			"include": "#singleStrings"
		}
	],
	"repository": {
		"keywords": {
			"patterns": [{
				"name": "keyword.control.mi",
				"match": "\\b(showBg|leave|showCh|playBgm|pauseBgm|resumeBgm|removeBg|showCg|removeCg|showChoose|showInput|showEffect|removeEffect)\\b"
			}]
		},
		"strings": {
			"name": "string.quoted.double.mi",
			"begin": "\"",
			"end": "\"",
			"patterns": [
				{
					"name": "constant.character.escape.mi",
					"match": "\\\\."
				}
			]
		},
		"singleStrings": {
			"name": "string.quoted.single.mi",
			"begin": "'",
			"end": "'",
			"patterns": [
				{
					"name": "constant.character.escape.mi",
					"match": "\\\\."
				}
			]
		}
	},
	"scopeName": "source.mi"
}